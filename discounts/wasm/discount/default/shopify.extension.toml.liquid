api_version = "2026-01"

[[extensions]]
name = "t:name"
handle = "{{handle | replace: " ", "-" | downcase}}"
type = "function"
{% if uid %}uid = "{{ uid }}"{% endif %}
description = "t:description"

  [[extensions.targeting]]
  target = "cart.lines.discounts.generate.run"
  input_query = "cart_lines_discounts_generate_run.graphql"
