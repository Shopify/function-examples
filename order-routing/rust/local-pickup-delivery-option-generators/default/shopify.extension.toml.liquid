name = "{{name}}"
type = "local_pickup_delivery_option_generator"
{% if uid %}uid = "{{ uid }}"{% endif %}
api_version = "unstable"

[build]
command = "cargo build --target=wasm32-wasip1 --release"
path = "target/wasm32-wasi/release/{{name | replace: " ", "-" | downcase}}.wasm"
watch = [ "src/**/*.rs" ]

[ui.paths]
create = "/"
details = "/"
